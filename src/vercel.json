{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "api/index.js"
    },
    {
      "src": "/(?!api).*",
      "dest": "index.html"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url",
    "MONGO_DB_NAME": "@mongo_db_name",
    "JWT_SECRET_KEY": "@jwt_secret_key",
    "JWT_ALGORITHM": "HS256",
    "JWT_ACCESS_TOKEN_EXPIRE_MINUTES": "10080"
  }
}
